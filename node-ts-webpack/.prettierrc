{
    "useTabs": false,
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "none",
    "tabWidth": 4,
    "overrides": [
        {
            "files": "*.json",
            "options": {
                "printWidth": 35
            }
        },
        {
            "files": "*.yml",
            "options": {
                "tabWidth": 2
            }
        },
        {
            "files": "*.md",
            "options": {
                "tabWidth": 1
            }
        }
    ]
}
